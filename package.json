{
  "name": "ai-strategy-navigator-monorepo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:landing-page": "pnpm --filter @ai-strategy-navigator/landing-page dev",
    "build:landing-page": "pnpm --filter @ai-strategy-navigator/landing-page build",
    "start:landing-page": "pnpm --filter @ai-strategy-navigator/landing-page start",
    "lint:landing-page": "pnpm --filter @ai-strategy-navigator/landing-page lint",
    "typecheck:landing-page": "pnpm --filter @ai-strategy-navigator/landing-page typecheck",
    "genkit:dev": "genkit start -- tsx packages/landing-page/src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch packages/landing-page/src/ai/dev.ts"
  },
  "devDependencies": {
    "genkit-cli": "^1.8.0",
    "typescript": "^5"
  }
}